{
  "manifest_version": 2,
  "name": "PassWordSaver",
  "description": "Save Your Password",
  "version": "1.0",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "scripts": ["js/scripts/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon":"img/icon.png",
    "default_title":"Pluralsight Helper"
  },
  "permissions": [
    "storage",
    "tabs",
    "notifications",
    "contextMenus"
  ]
}

